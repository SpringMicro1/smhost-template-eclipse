{
  "name": "smhost-template-eclipse",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "buildlocal": "astro build && npm run buildlocal --prefix node_modules/smhost-template-scripts",
    "build": "astro build && npm run build --prefix node_modules/smhost-template-scripts",
    "preview": "astro preview",
    "astro": "astro",
    "enable": "npm run enable --prefix node_modules/smhost-template-scripts",
    "disable": "npm run disable --prefix node_modules/smhost-template-scripts",
    "prep": "npm run prep --prefix node_modules/smhost-template-scripts",
    "stop": "npm run stop --prefix node_modules/smhost-template-scripts",
    "check": "npm run check --prefix node_modules/smhost-template-scripts https://github.com/SpringMicro1/smhost-template-eclipse.git"
  },
  "dependencies": {
    "@astrojs/image": "^0.14.1",
    "@astrojs/mdx": "^0.16.0",
    "@astrojs/sitemap": "^1.0.1",
    "@astrojs/tailwind": "^3.0.1",
    "@fontsource/inter": "^4.5.15",
    "astro": "^2.0.11",
    "astro-feather-icons": "^1.0.2",
    "astro-icon": "^0.8.0",
    "astro-navbar": "^1.1.5",
    "astro-seo": "^0.7.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "sharp": "^0.31.3",
    "smhost-template-scripts": "^0.4.0"
  }
}
